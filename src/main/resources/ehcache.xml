<?xml version="1.0" encoding="UTF-8"?>

<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="ehcache.xsd"
         updateCheck="true" monitoring="autodetect"
         dynamicConfig="true"  name="Wyndham" >

   
    <defaultCache         
        eternal="true"         
        maxEntriesLocalHeap="50000"  
        maxBytesLocalOffHeap="1m"       
        overflowToOffHeap="true">        
    </defaultCache>    
    <cache     
    name="preaggregator"     
    eternal="true"     
    maxEntriesLocalHeap="50000"
    maxBytesLocalOffHeap="100M"     
    overflowToOffHeap="false">  
	<searchable keys="false" values="false">
        <searchAttribute name="pkey" expression="value.getKey()"/>
        <searchAttribute name="partnerid" expression="value.getPartnerId()"/>
        <searchAttribute name="aggregate_flag" expression="value.getAggrStatusId()"/>
        <searchAttribute name="threadid" expression="value.getThreadId()"/>
        
    </searchable>
    <terracotta>
   <nonstop immediateTimeout="false" timeoutMillis="30000">
     <timeoutBehavior type="noop" />
   </nonstop>
 </terracotta>     

</cache>
<cache     
    name="aggregator"     
    eternal="true"     
    maxEntriesLocalHeap="50000"
    maxBytesLocalOffHeap="100M"     
    overflowToOffHeap="false">  
	<searchable keys="false" values="true">
        <searchAttribute name="message_status" expression="value.getMESSAGE_STATUS()"/>
        <searchAttribute name="source_time_stamp" expression="value.getSOURCE_TIME_STAMP_long"/>
    </searchable>
   <terracotta>
   <nonstop immediateTimeout="false" timeoutMillis="30000">
     <timeoutBehavior type="noop" />
   </nonstop>
 </terracotta>     
</cache>

   
    <terracottaConfig         
        url="localhost:9510"   rejoin="true"       
        />
</ehcache>
